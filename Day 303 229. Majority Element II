class Solution {
    public List<Integer> majorityElement(int[] nums) {
        int n = nums.length;
        int c1 = 0, c2 = 0, v1 = 0, v2 = 0;
        for (int x : nums) {
            if (x == v1) c1++;
            else if (x == v2) c2++;
            else if (c1 == 0) {
                v1 = x;
                c1 = 1;
            } else if (c2 == 0) {
                v2 = x;
                c2 = 1;
            } else {
                c1--;
                c2--;
            }
        }
        c1 = 0;
        c2 = 0;
        for (int x : nums) {
            if (x == v1) c1++;
            else if (x == v2) c2++;
        }
        List<Integer> res = new ArrayList<>();
        if (c1 > n / 3) res.add(v1);
        if (c2 > n / 3) res.add(v2);
        return res;
    }
}
